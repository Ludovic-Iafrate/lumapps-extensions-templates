{
    "name": "template-react-ts",
    "version": "0.4.2",
    "private": true,
    "scripts": {
        "check": "tsc --noEmit",
        "check:typing": "yarn run check",
        "start": "run-p start:playground start:shipping-server",
        "start:playground": "react-scripts start",
        "start:shipping-server": "lumapps-shipping-server",
        "test": "react-scripts test --coverage"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!@lumx/icons|@lumx/react)"
        ]
    },
    "dependencies": {
        "@lumapps-extensions-playground/common": "^1.1.7",
        "@lumx/core": "^1.0.18",
