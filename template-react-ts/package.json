        "axios": "0.21.1",
        "lodash": "4.17.21",
        "lumapps-sdk-js": "^0.0.44",
        "moment": "^2.29.1",
        "moment-range": "^4.0.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-intl": "5.15.7",
        "react-redux": "7.2.4",
        "reselect": "4.0.0"
    },
    "devDependencies": {
        "@lumapps-extensions-playground/devenv": "^1.1.7",
